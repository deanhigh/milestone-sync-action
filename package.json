{
  "name": "multirepo-project-actions",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc --build",
    "package:milestone-push": "ncc build ./lib/milestone-push.js -o ./dist/milestone-push",
    "package:label-push": "ncc build ./lib/label-push.js -o ./dist/label-push",
    "package": "npm-run-all package:*",
    "test": "jest",
    "all": "npm-run-all build test package",
    "clean": "rimraf ./dist ./lib ./coverage ./node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deanhigh/multirepo-project-actions.git"
  },
  "bugs": {
    "url": "https://github.com/deanhigh/multirepo-project-actions/issues"
  },
  "homepage": "https://github.com/deanhigh/multirepo-project-actions#readme",
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.11.18",
    "@vercel/ncc": "^0.36.0",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.0.4",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "js-yaml": "^4.1.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ]
  }
